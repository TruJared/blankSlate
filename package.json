{
  "name": "astro-blank-slate",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run build:themes && astro dev",
    "start": "npm run build:themes && astro dev",
    "build": "npm run build:themes && astro check && astro build",
    "check": "npm run build:themes && astro check",
    "preview": "npm run build:themes && astro preview",
    "astro": "astro",
    "build:themes": "node ./src/utils/buildThemes.mjs",
    "nuke": "rm -rf .astro && rm -rf node_modules -v && npm i && npm run build:themes"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/mdx": "^3.1.5",
    "@astrojs/react": "^3.6.2",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.0",
    "@iconify-json/ph": "^1.2.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "astro": "^4.15.2",
    "astro-icon": "^1.1.1",
    "astro-robots-txt": "^1.0.0",
    "css": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-emoji-render": "^2.0.1",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0",
    "typescript-eslint": "^8.4.0"
  }
}
